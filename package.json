{
  "name": "formflow",
  "version": "0.1.3",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.6",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.42",
    "bwip-js": "^3.0.5",
    "classnames": "^2.3.1",
    "concurrently": "^7.2.1",
    "dexie": "^3.2.2",
    "dexie-export-import": "^1.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.1",
    "fast-levenshtein": "^3.0.0",
    "formik": "^2.2.9",
    "is-electron": "^2.2.1",
    "javascript-time-ago": "^2.5.4",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.2",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-time-ago": "^7.2.1",
    "react-window": "^1.8.7",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "co.quartzic.formflow",
    "productName": "Formflow",
    "copyright": "Copyright Â© 2022 Nathan Goller-Deitsch",
    "mac": {
      "icon": "./assets/icon.icns"
    },
    "win": {
      "icon": "./assets/icon.png"
    },
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:pack-mac": "npm run build && electron-builder --dir",
    "electron:dist": "npm run build && electron-builder --win --x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "jest": "^28.1.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24"
  }
}
